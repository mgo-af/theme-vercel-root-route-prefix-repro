{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "docs/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    { "src": "/vercelredirects(/.*)?", "dest": "/docs$1" },
    {
      "src": "/*",
      "status": 301,
      "headers": { "Location": "/vercelredirects/" }
    }
  ]
}
