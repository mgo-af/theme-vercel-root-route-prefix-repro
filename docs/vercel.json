{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    { "src": "/vercelredirects(/.*)?", "dest": "/public$1" },
    { "src": "/", "status": 301, "headers": { "Location": "/vercelredirects" } }
  ]
}
